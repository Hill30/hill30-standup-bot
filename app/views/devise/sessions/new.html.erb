<div class="signin-form">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="form-group">
      <%= f.email_field :email, placeholder: "email", autofocus: false, class: "form-control"  %>
    </div>
    <div class="form-group">
      <%= f.password_field :password, placeholder: "password", autocomplete: "off", class: "form-control" %>
    </div>
    <div class="msg-error-wrapper">
    <%  if flash[:alert].present?  %>
      <div class="msg-error">
        <%= content_tag(:p, flash[:alert])%>
      </div>
    <% end %>
    </div>
    <div class="button-wrapper">
      <%= f.submit "Log in", class: "btn btn-primary" %>
    </div>
  <% end %>
</div>
